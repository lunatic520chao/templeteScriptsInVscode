{
	// Place your snippets for html here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"style in html":{
		"prefix": "<style_css",
		"body": [
			"<style type=\"text/css\">",
            "\t$1",
			"</style>"
		],
		"description": "table tips"
	},
	"table in html":{
		"prefix": "<table",
		"body": [
			"<table border=\"1\" >",
			"<tr>",
			"\t<td>",
			"\t\t$1",
			"\t</td>",
			"</tr>",
			"</table>"
		],
		"description": "table tips"
	},
	"import js source in html": {
		"prefix": "importjs_file",
		"body": [
			"<script src=\"$1.js\" type=\"text/javascript\" charset=\"utf-8\"   ></script>"
		],
		"description": "import js source in html"
	},
	"import js statement in html": {
		"prefix": "importjs_state",
		"body": [
			"<script  type=\"text/javascript\" charset=\"utf-8\"   >",
			"\t$1",
			"</script>"
		],
		"description": "import js source in html"
	},
	"templete": {
		"prefix": "tempwithJSBase",
		"body": [
			"<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional//EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd\">",
			"<html xmlns=\"http://www.w3.org/1999/xhtml\">",
			"<head>",
			"\t<meta charset=\"UTF-8\">",
			"\t<title></title>",
			"\t<script type=\"text/javascript\" charset=\"utf-8\"   >",
			"\t\t$1",
			"\t</script>",
			"</head>",
			"<body>",
			"\t<script type=\"text/javascript\" charset=\"utf-8\"   >",
			"\t\t",
			"\t</script>",
			"</body>",
			"</html>"
		],
		"description": "templete of the html"
	},
	"templete jquery": {
		"prefix": "tempwithjQuery",
		"body": [
			"<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional//EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd\">",
			"<html xmlns=\"http://www.w3.org/1999/xhtml\">",
			"<head>",
			"\t<meta charset=\"UTF-8\">",
			"\t<title></title>",
			"\t<script src=\"jquery.js\" type=\"text/javascript\" charset=\"utf-8\"   ></script>",
			"</head>",
			"<body>",
			"\t<script type=\"text/javascript\">",
			"\t\t$1",
			"\t</script>",
			"</body>",
			"</html>"
		],
		"description": "templete of the html"
	},
	"helloworld":{
		"prefix": "hello",
		"body": [
			"<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional//EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd\">",
